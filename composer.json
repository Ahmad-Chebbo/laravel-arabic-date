{
    "name": "ahmad-chebbo/laravel-arabic-date",
    "description": "A Laravel package for handling Arabic dates and Islamic calendar conversions",
    "keywords": ["laravel", "arabic", "date", "islamic", "calendar", "hijri"],
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmad Chebbo",
            "email": "ahmad.m.chebbo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AhmadChebbo\\LaravelArabicDate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AhmadChebbo\\LaravelArabicDate\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AhmadChebbo\\LaravelArabicDate\\ArabicDateServiceProvider"
            ],
            "aliases": {
                "ArabicDate": "AhmadChebbo\\LaravelArabicDate\\Facades\\ArabicDate"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html build/coverage",
        "test-coverage-text": "vendor/bin/phpunit --coverage-text"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
